       <select\\n              class=\\\"form-control custom-select\\\"\\n              formControlName=\\\"City\\\"\\n            >\\n            <option value=\\\"\\\">Value</option>\\n            <option [value]=\\\"d.villagecode\\\" *ngFor=\\\"let d of getParties()?.at(i)?.get('villageList')?.value\\\">\\n              {{ d.villagenamee  }}\\n            </option>\\n            </select>\\n          </div>\\n          <div class=\\\"mb-3 col-md-3\\\">\\n            <p class=\\\"sub-text font-black\\\">HOBLI</p>\\n            <select class=\\\"form-control custom-select\\\" formControlName=\\\"Hobli\\\">\\n              <option value=\\\"\\\">Value</option>\\n            <option [value]=\\\"d.hoblicode\\\" *ngFor=\\\"let d of getParties()?.at(i)?.get('hobliList')?.value\\\">\\n              {{ d.hoblinamee  }}\\n            </option>\\n            </select>\\n          </div>\\n          <div class=\\\"col-md-3 mb-3\\\">\\n            <p class=\\\"sub-text font-black\\\">Pin Code*</p>\\n            <input\\n              class=\\\"form-control custom-input\\\"\\n              placeholder=\\\"Pin Code*\\\"\\n              formControlName=\\\"PinCode\\\"\\n              type=\\\"number\\\"\\n            />\\n          </div>\\n        </div>\\n        <hr style=\\\"color: grey\\\" />\\n      </ng-container>\\n      <div class=\\\"col-md-12 mb-4\\\">\\n        <p\\n          style=\\\"\\n            color: #0a429b;\\n            font-weight: bold;\\n            font-size: 14px;\\n            cursor: pointer;\\n          \\\"\\n          (click)=\\\"addMoreParty()\\\"\\n        >\\n          Add more party\\n        </p>\\n      </div>\\n    </div>\\n  </div>\\n<div class=\\\"schedule-actions d-flex justify-content-center pb-4\\\">\\n    <button class=\\\"btn s-btn\\\" (click)=\\\"saveCTA()\\\"\\n        [disabled]=\\\"partyDetailsForm.invalid && addCourtOrderForm.invalid\\\">Save</button>\\n    <button class=\\\"btn p-btn\\\" (click)=\\\"cancelCTA()\\\">Cancel</butt