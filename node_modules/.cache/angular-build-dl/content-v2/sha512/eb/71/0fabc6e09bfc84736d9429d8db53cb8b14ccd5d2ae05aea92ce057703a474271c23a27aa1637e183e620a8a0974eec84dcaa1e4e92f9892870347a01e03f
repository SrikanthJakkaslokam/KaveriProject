ut@3x.png\\\" style = \\\"width: 133px;\\\"> -->\\n                                    <label style=\\\"font-size: 16px;font-weight: bold;\\\">Property Usage</label><br>\\n                                    <label style=\\\"font-size: 16px;font-weight: bold;\\\">NON-AGRICULTURAL</label>\\n                                </div>\\n                                <div class=\\\"form-group col-md-1\\\"></div>\\n                                <div class=\\\"form-group col-md-2\\\">\\n                                    <label for=\\\"units\\\" style=\\\"font-size: 14px;font-weight: bold;\\\">Property Type</label>\\n                                    <select type=\\\"text\\\" name=\\\"Units\\\" class=\\\"form-control\\\"\\n                                        (change)=\\\"onPropertyTypeValuechange($event)\\\" required>\\n                                        <!-- <option selected disabled readonly>Value</option> -->\\n                                        <option *ngFor=\\\"let p of propertytype\\\" [value]=\\\"p.propertytypename\\\">\\n                                            {{p.propertytypename}}</option>\\n                                    </select>\\n                                </div>\\n                                <div class=\\\"form-group col-md-1\\\"></div>\\n                                <div class=\\\"form-group col-md-2\\\">\\n                                    <label for=\\\"totalArea\\\" style=\\\"font-size: 14px;font-weight: bold;\\\">Total\\n                                        Area</label><br>\\n                                    <label style=\\\"font-size: 16px;font-weight: bold;\\\">{{Unitcode}} SQ.M</label>\\n                                </div>\\n                                <div class=\\\"form-group col-md-1\\\"></div>\\n                            </div>\\n                        </div>\\n                    </div>\\n                </div>\\n            </div>\\n\\n        </div>\\n\\n\\n        <div class=\\\"animat